{
  "name": "twees",
  "version": "1.0.0",
  "description": "twees - social media web application",
  "scripts": {
    "install": "concurrently \"npm install --prefix server\" \"npm install --prefix client\"",
    "server-install": "npm install --prefix server",
    "client-install": "npm install --prefix client",
    "start": "concurrently \"npm start --prefix server\" \"npm start --prefix client\"",
    "server-dev": "npm run dev --prefix server",
    "server": "npm start --prefix server",
    "client": "npm start --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natnaelab/twitter-clone.git"
  },
  "author": "Natnael Abrham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/natnaelab/twitter-clone/issues"
  },
  "homepage": "https://github.com/natnaelab/twitter-clone#readme",
  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
